{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "@fuse": [
        "@fuse/"
      ],
      "lead-core": [
        "../node_modules/lead-core"
      ],
      "lead-core/*": [
        "../node_modules/lead-core/*"
      ],
      "lead-fuse": [
        "../node_modules/lead-fuse"
      ],
      "lead-fuse/*": [
        "../node_modules/lead-fuse/*"
      ],
      "lead-account": [
        "../node_modules/lead-account"
      ],
      "lead-account/*": [
        "./node_modules/lib-lead-account/lib/*"
      ],
      "lead-test": [
        "./areas/lead-test/lib/public_api"
      ]
    },
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}